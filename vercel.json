{
  "version": 2,
  "deployment": {
    "protection": "none"
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/web/index.html"
    },
    {
      "source": "/web",
      "destination": "/web/index.html"
    },
    {
      "source": "/web/(.*)",
      "destination": "/web/$1"
    }
  ]
}