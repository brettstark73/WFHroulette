{
  "version": 2,
  "name": "wfh-roulette",
  "builds": [
    {
      "src": "web/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/web/(.*)",
      "dest": "/web/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web/index.html"
    }
  ],
  "functions": {
    "web/server.js": {
      "maxDuration": 10
    }
  }
}